@model Carrefour.Models.MenuModels
@{
    ViewBag.Title = "menu";
}

@if (Model.listItem.Count != 0)
{

    for (int i = 0; i < Model.listItem.Count; i++)
    {




        <li class="nav-item">
            <a href="../@Model.listItem[i].controller/@Model.listItem[i].procedure" class="nav-link">
                <i class="nav-icon fas fa-th"></i>
                <p>
                    @Model.listItem[i].nome
                    
                </p>

            </a>
        </li>



    }
}
